//- index.jade
extends ./layout.jade
block row-content
    .row
        .col-lg-4.col-md-4.col-sm-12
            each user in users
                a(href="/user/#{user._id}")
                    mixin card_min(user)
        .col-lg-8.col-md-8.col-sm-12(style="text-align:center;")
            canvas#graph_line
    div#graph_data(style="display:none;")
       | #{JSON.stringify(graph_data)}
block scripts
    script(type='text/javascript', src='/js/graph.js')