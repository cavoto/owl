//- index.jade
extends ./layout.jade
block row-content
    .row
        .col-lg-3.col-md-3.col-sm-12
            each user in users
                a(href="/user/#{user._id}")
                    //mixin card(user)
                    mixin card_min(user)
        .col-lg-9.col-md-9.col-sm-12(style="text-align:center;")
            canvas#graph_line
    div#graph_data(style="display:none;")
       | #{JSON.stringify(graph_data)}
block scripts
    script(type='text/javascript', src='/js/graph.js')