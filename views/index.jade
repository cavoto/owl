//- index.jade
extends ./layout.jade
block row-content
    div.container         
        .row
            .col-lg-4.col-md-4.col-sm-12
                each user in users
                    .col-lg-12.col-md-12.col-sm-12
                        .col-lg-8.col-md-8.col-sm-8
                            | #{user.user}
                        .col-lg-4.col-md-4.col-sm-4
                            mixin sentiment(user.sentiment)